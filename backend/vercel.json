{
  "version": 2,
  "builds": [
    {
      "src": "api.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.11"
      }
    },
    {
      "src": "api_flask.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/email/classify",
      "dest": "/api_flask.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api.py"
    }
  ]
}
